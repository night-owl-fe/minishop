.m-toast-container {
  position: fixed;
  z-index: 2010;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.m-toast {
  bottom: 200rpx;
  height: 99rpx;
  padding-left: 46rpx;
  padding-right: 46rpx;
  border-radius: 11rpx;
  font-size: 26rpx;
  color: #ffffff;
  background: rgba(0, 0, 0, 0.71);
}
