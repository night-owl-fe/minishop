/* 页面大标题 */
.title {
  display: block;
  padding: 36px 0 24px;
  font-weight: 500;
  color: #333;
}

/* 小程序样式 */
.program {
  width: 100%;
}

.program .program-text {
  overflow: hidden;
}

.program .program-logo {
  margin-left: 30px;
  width: 100px;
  height: 100px;
  border-radius: 8px;
}

.program .program-title {
  padding-right: 20px;
}

.program .program-des {
  padding-top: 12px;
}
